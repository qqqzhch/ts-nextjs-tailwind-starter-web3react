    "@web3-react/core": "^8.2.2",
    "@web3-react/metamask": "^8.2.3",
    "@web3-react/network": "^8.2.2",
    "@web3-react/walletconnect": "^8.2.2",
    "@web3-react/walletconnect-v2": "^8.5.0",
    "clsx": "^1.2.1",
    "flowbite": "^1.8.1",
